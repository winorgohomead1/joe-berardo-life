"overlay\" checked condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color:\" name=\"overlayColor\" value=\"#efefef\" condition=\"overlay && bg.type == 'image'\">\n        <input type=\"range\" inline title=\"Opacity:\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type == 'image'\">\n    </mbr-parameters>\n    <!--  -->\n    <!-- Overlay -->\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <!-- Container -->\n    <div class=\"container\">\n        <!-- Content -->\n        <div class=\"mbr-row mbr-jc-c\">\n            <!-- 1 -->\n            <div class=\"mbr-col-sm-12 mbr-col-md-3 align-center\">\n                <div class=\"item mbr-p-3\">\n                    <div class=\"item-icon mbr-pb-2\" mbr-if=\"showBlocksIcon\">\n                        <span mbr-icon class=\"amp-iconfont fa-clock-o fa\"></span>\n                    </div>\n                    <h3 class=\"item-title mbr-fonts-style mbr-pb-1 mbr-semibold\" mbr-bold mbr-theme-style=\"display-5\" mbr-if=\"showBlocksTitle\">20</h3>\n                    <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showBlocksSubtitle\">Years Of Experience</p>\n                </div>\n            </div>\n            <!-- 2 -->\n            <div class=\"mbr-col-sm-12 mbr-col-md-3 align-center\" mbr-if=\"blocksCount > 1\">\n                <div class=\"item mbr-p-3\">\n                    <div class=\"item-icon mbr-pb-2\" mbr-if=\"showBlocksIcon\">\n                        <span mbr-icon class=\"amp-iconfont fa-folder fa\"></span>\n                    </div>\n                    <h3 class=\"item-title mbr-fonts-style mbr-pb-1 mbr-semibold\" mbr-theme-style=\"display-5\" mbr-if=\"showBlocksTitle\">&gt;5000</h3>\n                    <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showBlocksSubtitle\">Successful Projects</p>\n                </div>\n            </div> \n            <!-- 3 -->\n            <div class=\"mbr-col-sm-12 mbr-col-md-3 align-center\" mbr-if=\"blocksCount > 2\">\n                <div class=\"item mbr-p-3\">\n                    <div class=\"item-icon mbr-pb-2\" mbr-if=\"showBlocksIcon\">\n                        <span mbr-icon class=\"amp-iconfont fa-hand-o-up fa\"></span>\n                    </div>\n                    <h3 class=\"item-title mbr-fonts-style mbr-pb-1 mbr-semibold\" mbr-theme-style=\"display-5\" mbr-if=\"showBlocksTitle\">&gt;500</h3>\n                    <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showBlocksSubtitle\">Team Members</p>\n                </div>\n            </div>\n            <!-- 4 -->\n            <div class=\"mbr-col-sm-12 mbr-col-md-3 align-center\" mbr-if=\"blocksCount > 3\">\n                <div class=\"item mbr-p-3\">\n                    <div class=\"item-icon mbr-pb-2\" mbr-if=\"showBlocksIcon\">\n                        <span mbr-icon class=\"amp-iconfont fa-child fa\"></span>\n                    </div>\n                    <h3 class=\"item-title mbr-fonts-style mbr-pb-1 mbr-semibold\" mbr-theme-style=\"display-5\" mbr-if=\"showBlocksTitle\">10000</h3>\n                    <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showBlocksSubtitle\">Happy Clients</p>\n                </div>\n            </div>\n        </div>\n    </div>\n\n\n</section>",
